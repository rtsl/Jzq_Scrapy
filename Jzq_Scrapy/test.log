                  u'\u3000\u3000\u4e24\u56fd\u603b\u7406\u9ad8\u5ea6\u8bc4\u4ef7\u4e2d\u4fc4\u5168\u9762\u6218\u7565\u534f\u4f5c\u4f19\u4f34\u5173\u7cfb\u8fdb\u5165\u65b0\u9636\u6bb5\u4ee5\u6765\u7684\u53d1\u5c55\u6210\u679c\uff0c\u8ba4\u4e3a\u7ee7\u7eed\u4fdd\u6301\u4e2d\u4fc4\u5173\u7cfb\u9ad8\u6c34\u5e73\u53d1\u5c55\u3001\u6269\u5927\u548c\u6df1\u5316\u5168\u65b9\u4f4d\u52a1\u5b9e\u5408\u4f5c\uff0c\u7b26\u5408\u4e24\u56fd\u4eba\u6c11\u7684\u6839\u672c\u5229\u76ca\uff0c\u7b26\u5408\u7ef4\u62a4\u5730\u533a\u53ca\u4e16\u754c\u548c\u5e73\u7a33\u5b9a\u7684\u9700\u8981\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5f3a\u8c03\uff0c\u65e0\u8bba\u56fd\u9645\u548c\u5730\u533a\u5f62\u52bf\u5982\u4f55\u53d8\u5316\uff0c\u90fd\u5c06\u7ee7\u7eed\u628a\u52a0\u5f3a\u76f8\u4e92\u5173\u7cfb\u4f5c\u4e3a\u672c\u56fd\u5916\u4ea4\u4f18\u5148\u65b9\u5411\uff0c\u575a\u5b9a\u652f\u6301\u5bf9\u65b9\u7ef4\u62a4\u672c\u56fd\u4e3b\u6743\u3001\u5b89\u5168\u3001\u53d1\u5c55\u7b49\u6838\u5fc3\u5229\u76ca\uff0c\u589e\u8fdb \u4e24\u56fd\u4eba\u6c11\u7684\u4e86\u89e3\u548c\u53cb\u8c0a\uff0c\u5bc6\u5207\u5728\u56fd\u9645\u548c\u5730\u533a\u4e8b\u52a1\u4e2d\u7684\u534f\u8c03\u914d\u5408\uff0c\u628a\u53cc\u65b9\u9ad8\u6c34\u5e73\u7684\u653f\u6cbb\u5173\u7cfb\u4f18\u52bf\u8f6c\u5316\u4e3a\u66f4\u591a\u52a1\u5b9e\u3001\u56fd\u9645\u3001\u4eba\u6587\u9886\u57df\u5408\u4f5c\u6210\u679c\uff0c\u5171\u540c\u5e94\u5bf9\u5916\u90e8\u73af\u5883\u7ed9\u4e24\u56fd \u5171\u540c\u632f\u5174\u5e26\u6765\u7684\u98ce\u9669\u548c\u6311\u6218\uff0c\u5171\u540c\u4fc3\u8fdb\u4e16\u754c\u7684\u548c\u5e73\u3001\u53d1\u5c55\u548c\u7e41\u8363\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u9ad8\u5ea6\u8bc4\u4ef720\u5e74\u6765\u4e2d\u4fc4\u603b\u7406\u5b9a\u671f\u4f1a\u6664\u673a\u5236\u53ca\u5176\u4e0b\u8bbe\u7684\u4e2d\u4fc4\u6295\u8d44\u5408\u4f5c\u59d4\u5458\u4f1a\u3001\u4e2d\u4fc4\u80fd\u6e90\u5408\u4f5c\u59d4\u5458\u4f1a\u3001\u4e2d\u4fc4\u4eba\u6587\u5408\u4f5c\u59d4\u5458\u4f1a\u3001\u4e2d\u4fc4\u603b\u7406\u5b9a\u671f\u4f1a\u6664\u59d4\u5458\u4f1a\u5728 \u6269\u5927\u548c\u6df1\u5316\u4e24\u56fd\u5404\u9886\u57df\u5408\u4f5c\u65b9\u9762\u53d1\u6325\u7684\u7edf\u7b79\u89c4\u5212\u548c\u6307\u5bfc\u63a8\u52a8\u4f5c\u7528\uff0c\u5c06\u4e0d\u65ad\u4f18\u5316\u548c\u5b8c\u5584\u603b\u7406\u5b9a\u671f\u4f1a\u6664\u673a\u5236\uff0c\u5728\u4e1d\u7ef8\u4e4b\u8def\u7ecf\u6d4e\u5e26\u5efa\u8bbe\u4e0e\u6b27\u4e9a\u7ecf\u6d4e\u8054\u76df\u5efa\u8bbe\u5bf9\u63a5\u6846\u67b6\u5185\u5f00\u5c55 \u5408\u4f5c\uff0c\u63a8\u52a8\u4e24\u56fd\u5728\u7ecf\u8d38\u3001\u6295\u8d44\u3001\u80fd\u6e90\u3001\u4eba\u6587\u7b49\u5404\u9886\u57df\u5408\u4f5c\u53d6\u5f97\u66f4\u591a\u5b9e\u9645\u6210\u679c\uff0c\u4fc3\u8fdb\u4e24\u56fd\u5171\u540c\u53d1\u5c55\u632f\u5174\u3002',
                  u'\u3000\u3000\u4e8c',
                  u'\u3000\u3000\u53cc\u65b9\u5c06\u5171\u540c\u52aa\u529b\uff0c\u91c7\u53d6\u6709\u9488\u5bf9\u6027\u7684\u63aa\u65bd\uff0c\u63a8\u52a8\u4e24\u56fd\u52a1\u5b9e\u5408\u4f5c\u6301\u7eed\u5feb\u901f\u53d1\u5c55\u3002\u4e3a\u6b64\uff0c\u53cc\u65b9\u5546\u5b9a:',
                  u'\u3000\u3000\u2014\u2014\u91c7\u53d6\u5207\u5b9e\u63aa\u65bd\u4fc3\u8fdb\u53cc\u8fb9\u8d38\u6613\u589e\u957f\uff0c\u9010\u6b65\u4f18\u5316\u8d38\u6613\u7ed3\u6784\uff0c\u63d0\u9ad8\u975e\u8d44\u6e90\u6027\u3001\u9ad8\u6280\u672f\u3001\u521b\u65b0\u4ea7\u54c1\u7684\u6bd4\u91cd\uff1b\u63d0\u5347\u8d38\u6613\u4fbf\u5229\u5316\u6c34\u5e73\uff0c\u4fdd\u969c\u5546\u54c1\u5e02\u573a\u76f8\u4e92\u5f00\u653e\uff0c\u79ef\u6781\u53d1\u5c55\u8de8\u5883\u7535\u5b50\u5546\u52a1\u3002',
                  u'\u3000\u3000\u2014\u2014\u6b22\u8fce\u4fc4\u5728\u534e\u8bbe\u7acb\u4fc4\u7f57\u65af\u8d38\u6613\u4e2d\u5fc3\uff0c\u91cd\u70b9\u5c55\u793a\u63a8\u4ecb\u4fc4\u4ea7\u54c1\uff0c\u52a0\u5927\u6210\u4ea4\u529b\u5ea6\uff1b\u652f\u6301\u4fc4\u5b9e\u65bd\u8fdb\u53e3\u66ff\u4ee3\u653f\u7b56\uff0c\u53cc\u65b9\u53ef\u5f00\u5c55\u76f8\u5173\u9886\u57df\u7684\u8d38\u6613\u548c\u672c\u571f\u5316\u751f\u4ea7\u5408\u4f5c\uff1b\u79ef\u6781\u8bc4\u4ef7\u4e2d\u56fd\u5546\u52a1\u90e8\u4e0e\u4fc4\u7f57\u65af\u7ecf\u6d4e\u53d1\u5c55\u90e8\u7b7e\u7f72\u300a\u5173\u4e8e\u4fc3\u8fdb\u53cc\u8fb9\u8d38\u6613\u7684\u8c05\u89e3\u5907\u5fd8\u5f55\u300b\u3002',
                  u'\u3000\u3000\u2014\u2014\u52a0\u5f3a\u8d22\u7ecf\u9886\u57df\u5408\u4f5c\uff0c\u5728\u4e24\u56fd\u8d22\u653f\u90e8\u7b7e\u7f72\u7684\u5408\u4f5c\u5907\u5fd8\u5f55\u6846\u67b6\u5185\uff0c\u52a0\u5f3a\u5728\u591a\u8fb9\u56fd\u9645\u8bba\u575b\u548c\u7ec4\u7ec7\u4e2d\u7684\u653f\u7b56\u534f\u8c03\uff0c\u62d3\u5c55\u9884\u7b97\u3001\u56fd\u5e93\u3001\u503a\u5238\u53d1\u884c\u7b49\u53cc\u8fb9\u52a1\u5b9e\u5408\u4f5c\u7684\u8303\u56f4\u3002\u63a8\u52a8\u4e24\u56fd\u91d1\u878d\u673a\u6784\u53c2\u4e0e\u4e9a\u6d32\u57fa\u7840\u8bbe\u65bd\u6295\u8d44\u94f6\u884c\u3001\u91d1\u7816\u56fd\u5bb6\u65b0\u5f00\u53d1\u94f6\u884c\u3001\u4e1d\u8def\u57fa\u91d1\u548c\u4e0a\u6d77\u5408\u4f5c\u7ec4\u7ec7\u94f6\u884c\u8054\u5408\u4f53\u7684\u4e1a\u52a1\u3002',
                  u'\u3000\u3000\u2014\u2014\u5bf9\u4e24\u56fd\u592e\u884c\u4e8e2015\u5e7410\u6708\u7b7e\u7f72\u7684\u5408\u4f5c\u8c05\u89e3\u5907\u5fd8\u5f55\u8868\u793a\u6b22\u8fce\uff0c\u652f\u6301\u4e24\u56fd\u592e\u884c\u5728\u5907\u5fd8\u5f55\u6846\u67b6\u5185\u6df1\u5316\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u2014\u2014\u52a0\u5f3a\u91d1\u878d\u9886\u57df\u5408\u4f5c\uff0c\u4e3a\u6269\u5927\u4e24\u56fd\u94f6\u884c\u3001\u652f\u4ed8\u7cfb\u7edf\u3001\u4fdd\u9669\u673a\u6784\u548c\u5176\u4ed6\u91d1\u878d\u673a\u6784\u5408\u4f5c\u521b\u9020\u6709\u5229\u6761\u4ef6\u3002',
                  u'\u3000\u3000\u2014\u2014\u6269\u5927\u8fdb\u51fa\u53e3\u94f6\u884c\u548c\u51fa\u53e3\u4fdd\u9669\u673a\u6784\u7684\u534f\u4f5c\uff0c\u652f\u6301\u548c\u4fc3\u8fdb\u53cc\u8fb9\u7ecf\u8d38\u5408\u4f5c\u53d1\u5c55\uff0c\u52a0\u5f3a\u4e24\u56fd\u94f6\u884c\u5728\u8d38\u6613\u548c\u9879\u76ee\u878d\u8d44\u65b9\u9762\u7684\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u2014\u2014\u6269\u5927\u672c\u5e01\u7ed3\u7b97\u53ca\u5176\u4f7f\u7528\u8303\u56f4\u3002\u53cc\u65b9\u5c06\u5c31\u5728\u4e24\u56fd\u592e\u884c\u5408\u4f5c\u8c05\u89e3\u5907\u5fd8\u5f55\u6846\u67b6\u4e0b\u6269\u5927\u592e\u884c\u95f4\u672c\u5e01\u4e92\u6362\u4f7f\u7528\u3001\u5408\u4f5c\u5f00\u53d1\u4eba\u6c11\u5e01/\u5362\u5e03\u91d1\u878d\u5de5\u5177\u5e02\u573a\u3001\u4e2d\u4fc4\u6295\u8d44\u8005\u76f8\u4e92\u53c2\u4e0e\u4e24\u56fd\u8d27\u5e01\u548c\u91d1\u878d\u5e02\u573a\u6d3b\u52a8\u3001\u652f\u6301\u4fc4\u7f57\u65af\u53d1\u5c55\u4eba\u6c11\u5e01\u79bb\u5cb8\u5e02\u573a\u3001\u5f00\u5c55\u652f\u4ed8\u7cfb\u7edf\u9886\u57df\u5408\u4f5c\u7b49\u4fdd\u6301\u79ef\u6781\u5bf9\u8bdd\u3002',
                  u'\u3000\u3000\u2014\u2014\u63d0\u5347\u519c\u4e1a\u5408\u4f5c\u6c34\u5e73\uff0c\u6269\u5927\u519c\u4ea7\u54c1\u8d38\u6613\uff0c\u5728\u786e\u4fdd\u5b89\u5168\u7684\u524d\u63d0\u4e0b\u4fdd\u969c\u52a8\u690d\u7269\u4ea7\u54c1\u76f8\u4e92\u8fdb\u5165\u5bf9\u65b9\u5e02\u573a\u3002\u5728\u52a8\u690d\u7269\u68c0\u9a8c\u68c0\u75ab\u3001\u6e14\u4e1a\u548c\u6c34\u4ea7\u517b\u6b96\u65b9\u9762\u5f00\u5c55\u5408\u4f5c\u3002\u652f\u6301\u4e24\u56fd\u6bd7\u90bb\u5730\u533a\u6df1\u5316\u519c\u4e1a\u5f00\u53d1\u5408\u4f5c\uff0c\u63a8\u8fdb\u5b9e\u65bd\u5728\u4fc4\u7f57\u65af\u5883\u5185\u7684\u519c\u4e1a\u6295\u8d44\u5408\u4f5c\u9879\u76ee\u3002',
                  u'\u3000\u3000\u2014\u2014\u5728\u843d\u5b9e\u300a2013\uff0d2017\u5e74\u4e2d\u4fc4\u822a\u5929\u5408\u4f5c\u5927\u7eb2\u300b\u7684\u57fa\u7840\u4e0a\uff0c\u6269\u5927\u5e76\u6df1\u5316\u957f\u671f\u4e92\u5229\u5408\u4f5c\uff0c\u91cd\u70b9\u5173\u6ce8\u5728\u706b\u7bad\u53d1\u52a8\u673a\u3001\u822a\u5929\u7535\u5b50\u5143\u5668\u4ef6\u3001\u5bf9\u5730\u89c2\u6d4b\u3001\u6708\u7403\u4e0e\u6df1\u7a7a\u63a2\u6d4b\u3001\u536b\u661f\u5bfc\u822a\u7b49\u9886\u57df\u5408\u4f5c\u3002\u4e0a\u8ff0\u5408\u4f5c\u6709\u5229\u4e8e\u4fc3\u8fdb\u4e24\u56fd\u5de5\u4e1a\u3001\u79d1\u6280\u7b49\u9ad8\u65b0\u6280\u672f\u9886\u57df\u7684\u53d1\u5c55\u3002',
                  u'\u3000\u3000\u2014\u2014\u79ef\u6781\u5f00\u5c55\u5de5\u4e1a\u5408\u4f5c\uff0c\u5e76\u4e3a\u6b64\u51b3\u5b9a\u5728\u4e2d\u4fc4\u603b\u7406\u5b9a\u671f\u4f1a\u6664\u59d4\u5458\u4f1a\u6846\u67b6\u4e0b\u8bbe\u7acb\u5de5\u4e1a\u5408\u4f5c\u5206\u59d4\u4f1a\uff0c\u540c\u65f6\u64a4\u9500\u539f\u6709\u6c11\u7528\u822a\u7a7a\u5408\u4f5c\u5206\u59d4\u4f1a\u3002',
                  u'\u3000\u3000\u2014\u2014\u7ee7\u7eed\u63a8\u8fdb\u548c\u6df1\u5316\u6c11\u7528\u822a\u7a7a\u9886\u57df\u5408\u4f5c\uff0c\u63a8\u52a8\u843d\u5b9e\u5927\u578b\u9879\u76ee\uff0c\u5305\u62ec\u8054\u5408\u7814\u5236\u9879\u76ee\uff0c\u6269\u5927\u5728\u822a\u7a7a\u53d1\u52a8\u673a\u9886\u57df\u5408\u4f5c\uff0c\u52a0\u5f3a\u6750\u6599\u3001\u5de5\u827a\u7b49\u79d1\u6280\u9886\u57df\u548c\u9002\u822a\u9886\u57df\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u2014\u2014\u7ee7\u7eed\u52a0\u5f3a\u4e24\u56fd\u6d77\u5173\u5408\u4f5c\uff0c\u6709\u6548\u843d\u5b9e\u201c\u7eff\u8272\u901a\u9053\u201d\u3001\u76d1\u7ba1\u7ed3\u679c\u4e92\u8ba4\u3001\u96c6\u88c5\u7bb1\u4f9b\u5e94\u94fe\u5b89\u5168\u4fdd\u969c\u9879\u76ee\uff0c\u5728\u6d77\u5173\u5e72\u90e8\u57f9\u8bad\u3001\u77e5\u8bc6\u4ea7\u6743\u4fdd\u62a4\u3001\u6253\u51fb\u8fdd\u53cd\u6d77\u5173\u6cd5\u884c\u4e3a\u3001\u52a0\u5f3a\u8fb9\u5883\u6d77\u5173\u5408\u4f5c\u65b9\u9762\u52a0\u5f3a\u534f\u4f5c\u3002',
                  u'\u3000\u3000\u2014\u2014\u6df1\u5316\u79d1\u6280\u4e0e\u521b\u65b0\u9886\u57df\u5408\u4f5c\uff0c\u63a8\u52a8\u5728\u4f18\u5148\u9886\u57df\u5b9e\u65bd\u79d1\u6280\u5408\u4f5c\u9879\u76ee\uff0c\u63a8\u52a8\u5728\u6838\u805a\u53d8\u548c\u9ad8\u80fd\u7269\u7406\u7b49\u9886\u57df\u53d1\u5c55\u5168\u9762\u79d1\u7814\u57fa\u7840\u8bbe\u65bd\uff0c\u652f\u6301\u57fa\u7840\u7814\u7a76\u548c\u5e94\u7528\u7814\u7a76\u3002',
                  u'\u3000\u3000\u2014\u2014\u6df1\u5316\u73af\u4fdd\u5408\u4f5c\uff0c\u5bc6\u5207\u5728\u4e0a\u6d77\u5408\u4f5c\u7ec4\u7ec7\u3001\u91d1\u7816\u56fd\u5bb6\u7b49\u6846\u67b6\u4e0b\u7684\u76f8\u4e92\u534f\u4f5c\u3002\u79ef\u6781\u63a2\u7d22\u63a8\u8fdb\u5728\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u548c\u4fc4\u7f57\u65af\u8054\u90a6\u5173\u4e8e\u4e1d\u7ef8\u4e4b\u8def\u7ecf\u6d4e\u5e26\u5efa\u8bbe\u548c\u6b27 \u4e9a\u7ecf\u6d4e\u8054\u76df\u5efa\u8bbe\u5bf9\u63a5\u5408\u4f5c\u7684\u8054\u5408\u58f0\u660e\u300b\u6846\u67b6\u4e0b\u7684\u73af\u4fdd\u5408\u4f5c\u3002\u53cc\u65b9\u5f3a\u8c03\u5df2\u5f00\u59cb\u987a\u5229\u6267\u884c\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u653f\u5e9c\u548c\u4fc4\u7f57\u65af\u8054\u90a6\u653f\u5e9c\u5173\u4e8e\u4fdd\u62a4\u5019\u9e1f\u53ca\u5176\u6816\u606f\u5730\u73af\u5883\u7684\u534f \u5b9a\u300b\uff0c\u5c06\u7ee7\u7eed\u5728\u4fdd\u62a4\u751f\u7269\u591a\u6837\u6027\u548c\u8de8\u754c\u81ea\u7136\u4fdd\u62a4\u533a\u65b9\u9762\u5f00\u5c55\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u2014\u2014\u8fdb\u4e00\u6b65\u6df1\u5316\u6797\u4e1a\u4ea4\u6d41\u4e0e\u5408\u4f5c\uff0c\u7ee7\u7eed\u52a0\u5f3a\u5728\u8fb9\u5883\u68ee\u6797\u9632\u706b\u8054\u9632\u3001\u68ee\u6797\u8d44\u6e90\u53ef\u6301\u7eed\u7ecf\u8425\u5229\u7528\u3001\u91ce\u751f\u52a8\u690d\u7269\u7269\u79cd\u4fdd\u62a4\u3001\u6253\u51fb\u6728\u6750\u975e\u6cd5\u91c7\u4f10\u548c\u76f8\u5173\u8d38\u6613\u3001\u5efa\u8bbe\u6728\u6750\u6df1\u52a0\u5de5\u56ed\u533a\u7b49\u9886\u57df\u7684\u5b9a\u671f\u6c9f\u901a\u4e0e\u534f\u4f5c\u3002',
                  u'\u3000\u3000\u2014\u2014\u52a0\u5f3a\u53cc\u65b9\u5728\u4f1a\u5c55\u9886\u57df\u7684\u5408\u4f5c\uff0c\u6839\u636e\u300a\u5173\u4e8e\u7b79\u5907\u548c\u4e3e\u529e\u4e2d\u4fc4\u535a\u89c8\u4f1a\u7684\u8c05\u89e3\u5907\u5fd8\u5f55\u300b\uff0c\u5171\u540c\u529e\u597d2016\u5e74\u5728\u4fc4\u7f57\u65af\u53f6\u5361\u6377\u7433\u5821\u5e02\u4e3e\u529e\u7684\u7b2c\u4e09\u5c4a\u4e2d\u4fc4\u535a\u89c8\u4f1a\uff0c\u652f\u6301\u4e24\u56fd\u4f01\u4e1a\u76f8\u4e92\u53c2\u52a0\u5728\u5bf9\u65b9\u56fd\u5bb6\u4e3e\u529e\u7684\u5c55\u4f1a\u3001\u8bba\u575b\u7b49\u6d3b\u52a8\u3002',
                  u'\u3000\u3000\u2014\u2014\u52a0\u5feb\u63a8\u8fdb\u300a\u4e2d\u56fd\u4e0e\u6b27\u4e9a\u7ecf\u6d4e\u8054\u76df\u7ecf\u8d38\u5408\u4f5c\u4f19\u4f34\u534f\u5b9a\u300b\u8c08\u5224\u8fdb\u7a0b\u3002',
                  u'\u3000\u3000\u2014\u2014\u5728\u4f7f\u7528\u4fc4\u7f57\u65af\u8fdc\u4e1c\u6e2f\u53e3\u7b49\u4ea4\u901a\u8fd0\u8f93\u57fa\u7840\u8bbe\u65bd\u53d1\u5c55\u4e2d\u4fc4\u8fc7\u5883\u8fd0\u8f93\u53ca\u9646\u6d77\u8054\u8fd0\u65b9\u9762\u52a0\u5f3a\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u2014\u2014\u7ee7\u7eed\u5408\u4f5c\u5efa\u8bbe\u540c\u6c5f\uff0d\u4e0b\u5217\u5b81\u65af\u9614\u8036\u8de8\u5883\u94c1\u8def\u6865\u3002',
                  u'\u3000\u3000\u2014\u2014\u7ee7\u7eed\u63a8\u8fdb\u9ed1\u778e\u5b50\u5c9b\uff08\u5927\u4e4c\u82cf\u91cc\u5c9b\uff09\u8bbe\u7acb\u53e3\u5cb8\u5de5\u4f5c\uff0c\u8054\u5408\u5f00\u653e\u5f00\u53d1\u9ed1\u778e\u5b50\u5c9b\uff08\u5927\u4e4c\u82cf\u91cc\u5c9b\uff09\u3002',
                  u'\u3000\u3000\u2014\u2014\u843d\u5b9e\u4e24\u56fd\u5143\u99962015\u5e749\u67083\u65e5\u5728\u5317\u4eac\u4f1a\u6664\u671f\u95f4\u89c1\u8bc1\u7b7e\u7f72\u7684\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u653f\u5e9c\u4e0e\u4fc4\u7f57\u65af\u8054\u90a6\u653f\u5e9c\u5173\u4e8e\u5728\u4e2d\u4fc4\u8fb9\u5883\u9ed1\u6cb3\u5e02\uff08\u4e2d\u56fd\uff09\u4e0e\u5e03\u62c9\u6208\u7ef4\u7533 \u65af\u514b\u5e02\uff08\u4fc4\u7f57\u65af\uff09\u4e4b\u95f4\u5efa\u8bbe\u3001\u4f7f\u7528\u3001\u7ba1\u7406\u548c\u7ef4\u62a4\u8de8\u9ed1\u9f99\u6c5f\uff08\u963f\u7a46\u5c14\u6cb3\uff09\u8de8\u5883\u7d22\u9053\u7684\u534f\u5b9a\u300b\u3001\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u653f\u5e9c\u4e0e\u4fc4\u7f57\u65af\u8054\u90a6\u653f\u5e9c\u5173\u4e8e\u4fee\u8ba21995\u5e746\u670826\u65e5 \u7b7e\u7f72\u7684\u3008\u4e2d\u4fc4\u5173\u4e8e\u5171\u540c\u5efa\u8bbe\u9ed1\u6cb3\uff0d\u5e03\u62c9\u6208\u7ef4\u7533\u65af\u514b\u9ed1\u9f99\u6c5f\uff08\u963f\u7a46\u5c14\u6cb3\uff09\u5927\u6865\u7684\u534f\u5b9a\u3009\u7684\u8bae\u5b9a\u4e66\u300b\u3002',
                  u'\u3000\u3000\u2014\u2014\u7ee7\u7eed\u5f00\u5c55\u6ee8\u6d77\u901a\u90531\u53f7\u548c\u6ee8\u6d77\u901a\u90532\u53f7\u8fc7\u5883\u8fd0\u8f93\u3002',
                  u'\u3000\u3000\u2014\u2014\u52a0\u5f3a\u5317\u65b9\u6d77\u822a\u9053\u5f00\u53d1\u5229\u7528\u5408\u4f5c\uff0c\u5f00\u5c55\u5317\u6781\u822a\u8fd0\u7814\u7a76\u3002',
                  u'\u3000\u3000\u2014\u2014\u7ee7\u7eed\u5b9e\u65bd\u83ab\u65af\u79d1\uff0d\u5580\u5c71\u9ad8\u901f\u94c1\u8def\u9879\u76ee\u3002',
                  u'\u3000\u3000\u2014\u2014\u8fdb\u4e00\u6b65\u52a0\u5f3a\u536b\u661f\u5bfc\u822a\u52a1\u5b9e\u5408\u4f5c\uff0c\u5728\u4e2d\u56fd\u5317\u6597\u7cfb\u7edf\u4e0e\u4fc4\u7f57\u65af\u683c\u6d1b\u7eb3\u65af\u7cfb\u7edf\u517c\u5bb9\u4e0e\u4e92\u64cd\u4f5c\u3001\u589e\u5f3a\u7cfb\u7edf\u4e0e\u5efa\u7ad9\u3001\u76d1\u6d4b\u8bc4\u4f30\u3001\u8054\u5408\u5e94\u7528\u7b49\u9886\u57df\u63a8\u52a8\u5b9e\u65bd\u6807\u5fd7\u6027\u5408\u4f5c\u9879\u76ee\u3002',
                  u'\u3000\u3000\u2014\u2014\u5728\u4e1d\u7ef8\u4e4b\u8def\u7ecf\u6d4e\u5e26\u5efa\u8bbe\u4e0e\u6b27\u4e9a\u7ecf\u6d4e\u8054\u76df\u5efa\u8bbe\u5bf9\u63a5\u6846\u67b6\u4e0b\u7814\u7a76\u4ea4\u901a\u57fa\u7840\u8bbe\u65bd\u548c\u7269\u6d41\u5408\u4f5c\u9879\u76ee\u3002',
                  u'\u3000\u3000\u4e09',
                  u'\u3000\u3000\u53cc\u65b9\u5f3a\u8c03\uff0c\u8981\u8fdb\u4e00\u6b65\u53d1\u6325\u4e2d\u4fc4\u6295\u8d44\u5408\u4f5c\u59d4\u5458\u4f1a\u91cd\u8981\u5e73\u53f0\u4f5c\u7528\uff0c\u6539\u5584\u4e24\u56fd\u6295\u8d44\u73af\u5883\uff0c\u79ef\u6781\u62d3\u5c55\u5408\u4f5c\u9886\u57df\uff0c\u52a0\u5f3a\u5bf9\u5916\u8d38\u6613\u653f\u7b56\u4ea4\u6d41\uff0c\u63d0\u9ad8\u653f\u5e9c\u5ba1\u6279\u6548\u7387\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5546\u5b9a\uff0c\u52a0\u5feb\u63a8\u8fdb\u548c\u843d\u5b9e\u5728\u80fd\u6e90\u548c\u8d44\u6e90\u5f00\u53d1\u3001\u751f\u4ea7\u52a0\u5de5\u3001\u88c5\u5907\u5236\u9020\u3001\u57fa\u7840\u8bbe\u65bd\u5efa\u8bbe\u7b49\u9886\u57df\u7684\u91cd\u5927\u6295\u8d44\u9879\u76ee\uff0c\u5229\u7528\u53cc\u65b9\u4e92\u8865\u4f18\u52bf\uff0c\u4fc3\u8fdb\u4ea7\u80fd\u5408\u4f5c\uff0c\u6301\u7eed\u63d0\u5347\u4e24\u56fd\u6295\u8d44\u5408\u4f5c\u7684\u89c4\u6a21\u548c\u6c34\u5e73\u3002',
                  u'\u3000\u3000\u4e24\u56fd\u603b\u7406\u5bf9\u53cc\u65b9\u4fdd\u9669\u9886\u57df\u5408\u4f5c\u53d6\u5f97\u7684\u6210\u679c\uff0c\u7279\u522b\u662f2015\u5e7411\u67089\u65e5\u4e2d\u56fd\u4fdd\u9669\u76d1\u7763\u7ba1\u7406\u59d4\u5458\u4f1a\u4e0e\u4fc4\u7f57\u65af\u8054\u90a6\u4e2d\u592e\u94f6\u884c\u7b7e\u7f72\u300a\u4e2d\u4fc4\u4fdd\u9669\u76d1\u7ba1\u5408\u4f5c\u8c05\u89e3\u5907\u5fd8\u5f55\u300b\u4e88\u4ee5\u79ef\u6781\u8bc4\u4ef7\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5c06\u91c7\u53d6\u63aa\u65bd\u5f00\u5c55\u4e24\u56fd\u4fdd\u9669\u5e02\u573a\u4fe1\u606f\u4ea4\u6362\u548c\u5bf9\u4fdd\u9669\u516c\u53f8\u6d3b\u52a8\u76d1\u7763\u7ba1\u7406\u7ecf\u9a8c\u4ea4\u6d41\u3002\u4e3a\u4fc3\u8fdb\u53cc\u8fb9\u7ecf\u8d38\u5408\u4f5c\u548c\u76f8\u4e92\u6295\u8d44\uff0c\u53cc\u65b9\u5c06\u52a0\u5f3a\u4fdd\u9669\u8d44\u91d1\u6295\u8d44\u3001\u5916\u8d38\u548c\u6295\u8d44\u6d3b\u52a8\u4fdd\u9669\u3001\u65c5\u6e38\u4fdd\u9669\u3001\u518d\u4fdd\u9669\u7b49\u9886\u57df\u7684\u4fdd\u9669\u652f\u6301\u548c\u5408\u4f5c\uff0c\u8054\u5408\u8fdb\u884c\u4e2d\u4fc4\u4fdd\u9669\u673a\u6784\u8bc4\u7ea7\u8981\u6c42\u7814\u7a76\u3002',
                  u'\u3000\u3000\u56db',
                  u'\u3000\u3000\u4e24\u56fd\u603b\u7406\u9ad8\u5ea6\u8bc4\u4ef7\u4e2d\u4fc4\u80fd\u6e90\u5408\u4f5c\u53d6\u5f97\u7684\u91cd\u8981\u6210\u679c\uff0c\u89c6\u80fd\u6e90\u5408\u4f5c\u4e3a\u53cc\u8fb9\u5173\u7cfb\u7684\u6218\u7565\u9886\u57df\uff0c\u63a8\u52a8\u53cc\u65b9\u52a0\u5f3a\u5305\u62ec\u5728\u53cc\u65b9\u5df2\u7b7e\u7f72\u7684\u653f\u5e9c\u95f4\u534f\u8bae\u6846\u67b6\u4e0b\u7684\u5168\u9762\u80fd\u6e90\u5408\u4f5c\u4f19\u4f34\u5173\u7cfb\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5c06\u7ee7\u7eed\u6df1\u5316\u77f3\u6cb9\u9886\u57df\u4e00\u63fd\u5b50\u5408\u4f5c\uff0c\u7a33\u6b65\u63a8\u8fdb\u5411\u4e2d\u56fd\u51fa\u53e3\u4fc4\u7f57\u65af\u5929\u7136\u6c14\uff0c\u5305\u62ec\u5229\u7528\u5408\u4f5c\u9879\u76ee\u4f9b\u5e94\u6db2\u5316\u5929\u7136\u6c14\u3002\u8fdb\u4e00\u6b65\u52a0\u5f3a\u7535\u529b\u3001\u7164\u70ad\u3001\u53ef\u518d\u751f\u80fd\u6e90\u3001\u80fd\u6e90\u6280\u672f\u88c5\u5907\u548c\u5de5\u7a0b\u670d\u52a1\u9886\u57df\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5c06\u672c\u7740\u4e92\u5229\u3001\u5e73\u7b49\u3001\u786e\u4fdd\u6838\u5b89\u5168\u7684\u539f\u5219\uff0c\u7ee7\u7eed\u63a8\u8fdb\u548c\u5e73\u5229\u7528\u6838\u80fd\u9886\u57df\u4e00\u63fd\u5b50\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u4e94',
                  u'\u3000\u3000\u53cc\u65b9\u9ad8\u5ea6\u8bc4\u4ef7\u542f\u52a8\u4e2d\u56fd\u4e1c\u5317\u5730\u533a\u548c\u4fc4\u7f57\u65af\u8fdc\u4e1c\u5730\u533a\u5730\u65b9\u5408\u4f5c\u7406\u4e8b\u4f1a\u7684\u5de5\u4f5c\uff0c\u5c06\u5145\u5206\u53d1\u6325\u8be5\u673a\u5236\u7684\u4f5c\u7528\uff0c\u79ef\u6781\u63a8\u52a8\u4e0a\u8ff0\u5730\u533a\u95f4\u5408\u4f5c\uff0c\u63d0\u9ad8\u5408\u4f5c\u8d28\u91cf\u548c\u5b9e\u9645\u6548\u76ca\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u652f\u6301\u5728\u4fc4\u7f57\u65af\u8fdc\u4e1c\u5730\u533a\u5f00\u5c55\u6e2f\u53e3\u7269\u6d41\u3001\u8d44\u6e90\u5f00\u53d1\u3001\u519c\u4ea7\u54c1\u52a0\u5de5\u3001\u79d1\u7814\u6559\u80b2\u3001\u65c5\u6e38\u53ca\u4ea4\u901a\u57fa\u7840\u8bbe\u65bd\u5efa\u8bbe\u7b49\u91cd\u70b9\u9886\u57df\u4e92\u5229\u5408\u4f5c\uff0c\u5171\u540c\u5b9e\u65bd\u91cd\u5927\u9879\u76ee\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u79ef\u6781\u63a8\u52a8\u4f01\u4e1a\u53c2\u4e0e\u4fc4\u7f57\u65af\u8fdc\u4e1c\u8de8\u8d8a\u5f0f\u53d1\u5c55\u533a\u548c\u7b26\u62c9\u8fea\u6c83\u65af\u6258\u514b\u81ea\u7531\u6e2f\u5f00\u53d1\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5bf9\u4e2d\u56fd\u957f\u6c5f\u4e2d\u4e0a\u6e38\u5730\u533a\u548c\u4fc4\u7f57\u65af\u4f0f\u5c14\u52a0\u6cb3\u6cbf\u5cb8\u8054\u90a6\u533a\u5408\u4f5c\u53d6\u5f97\u7684\u79ef\u6781\u8fdb\u5c55\u8868\u793a\u6ee1\u610f\uff0c\u613f\u5145\u5206\u5229\u7528\u597d\u8fd9\u4e00\u673a\u5236\uff0c\u8fdb\u4e00\u6b65\u53d1\u5c55\u4e24\u5730\u533a\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5c06\u7ee7\u7eed\u6269\u5927\u4e24\u56fd\u5730\u65b9\u5408\u4f5c\u7684\u5730\u57df\u8303\u56f4\uff0c\u63d0\u5347\u5408\u4f5c\u6c34\u5e73\uff0c\u63a8\u52a8\u4e24\u56fd\u5730\u65b9\u95f4\u5408\u4f5c\u6301\u7eed\u6df1\u5165\u53d1\u5c55\u3002',
                  u'\u3000\u3000\u516d',
                  u'\u3000\u3000\u4e24\u56fd\u603b\u7406\u9ad8\u5ea6\u8bc4\u4ef7\u4e2d\u4fc4\u4eba\u6587\u5408\u4f5c\u59d4\u5458\u4f1a\u5bcc\u6709\u6210\u6548\u7684\u5de5\u4f5c\uff0c\u6307\u51fa\u4eba\u6587\u5408\u4f5c\u5728\u592f\u5b9e\u4e24\u56fd\u5173\u7cfb\u793e\u4f1a\u548c\u6c11\u610f\u57fa\u7840\u65b9\u9762\u53d1\u6325\u7740\u91cd\u8981\u4f5c\u7528\uff0c\u5e76\u5c06\u81f4\u529b\u4e8e\u6269\u5927\u4eba\u6587\u4ea4\u6d41\uff0c\u63d0\u5347\u4e24\u56fd\u8f6f\u5b9e\u529b\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u79ef\u6781\u8bc4\u4ef72014\uff0d2015\u5e74\u4e3e\u529e\u7684\u4e2d\u4fc4\u9752\u5e74\u53cb\u597d\u4ea4\u6d41\u5e74\u6d3b\u52a8\u3002\u9752\u5e74\u5e74\u6d3b\u52a8\u4fc3\u8fdb\u4e86\u4e24\u56fd\u9752\u5e74\u5f80\u6765\u548c\u4e92\u4fe1\uff0c\u589e\u8fdb\u4e86\u7ecf\u9a8c\u4ea4\u6d41\uff0c\u5de9\u56fa\u4e86\u53cb\u8c0a\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5f3a\u8c03\uff0c\u5171\u540c\u7b79\u5907\u548c\u4e3e\u529e\u597d2016\uff0d2017\u5e74\u4e2d\u4fc4\u5a92\u4f53\u4ea4\u6d41\u5e74\u5c06\u4fc3\u8fdb\u4e24\u56fd\u5a92\u4f53\u95f4\u7684\u4ea4\u6d41\uff0c\u4e3a\u4e24\u56fd\u5173\u7cfb\u7ee7\u7eed\u5feb\u901f\u53d1\u5c55\u8425\u9020\u826f\u597d\u6c1b\u56f4\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u4e00\u81f4\u8ba4\u4e3a\uff0c\u5e94\u7ee7\u7eed\u5b9e\u65bd\u597d\u4e2d\u4fc42020\u5e7410\u4e07\u4eba\u7559\u5b66\u8ba1\u5212\uff0c\u7b79\u5efa\u597d\u6df1\u5733\u201c\u4e2d\u4fc4\u8054\u5408\u5927\u5b66\u201d\uff0c\u652f\u6301\u5efa\u7acb\u548c\u6269\u5927\u4e24\u56fd\u5404\u7c7b\u9ad8\u6821\u8054\u76df\u5efa\u8bbe\uff0c\u5171\u540c\u7814\u7a76\u63a8\u52a8\u53cc\u5b66\u4f4d\u9879\u76ee\uff0c\u4ee5\u53ca\u7535\u5b50\uff08\u8fdc\u7a0b\uff09\u6559\u5b66\u7b49\u5176\u4ed6\u5f62\u5f0f\u7684\u6559\u80b2\u5408\u4f5c\uff0c\u52a0\u5f3a\u6c49\u8bed\u548c\u4fc4\u8bed\u8bed\u8a00\u6559\u5b66\u5408\u4f5c\uff0c\u50a8\u5907\u66f4\u591a\u76f8\u5173\u4e13\u4e1a\u4eba\u624d\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5546\u5b9a\u5c06\u7ee7\u7eed\u529e\u597d\u6587\u5316\u8282\u3001\u4e2d\u4fc4\u6587\u5316\u5927\u96c6\uff0c\u589e\u8fdb\u4e24\u56fd\u827a\u672f\u548c\u4e13\u4e1a\u56e2\u4f53\u3001\u673a\u6784\u95f4\u7684\u8054\u7cfb\uff0c\u652f\u6301\u53cc\u65b9\u6587\u5316\u4e2d\u5fc3\u8fd0\u4f5c\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5c06\u5206\u4eab\u707e\u5bb3\u533b\u5b66\u9886\u57df\u7ecf\u9a8c\uff0c\u6df1\u5316\u536b\u751f\u9632\u75ab\u5408\u4f5c\uff0c\u52a0\u5f3a\u5728\u4f20\u67d3\u75c5\u9632\u63a7\u9886\u57df\u7684\u534f\u4f5c\uff0c\u5f00\u5c55\u73b0\u4ee3\u533b\u7597\u6280\u672f\u5e94\u7528\u4ea4\u6d41\uff0c\u4e3a\u5728\u536b\u751f\u9886\u57df\u5b9e\u65bd\u8054\u5408\u56fd\u53ef\u6301\u7eed\u53d1\u5c55\u76ee\u6807\u534f\u8c03\u884c\u52a8\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u91cd\u7533\u5c06\u6df1\u5316\u548c\u6269\u5927\u4f53\u80b2\u9886\u57df\u5408\u4f5c\uff0c\u4e3e\u529e\u597d\u4e2d\u4fc4\u9752\u5c11\u5e74\u8fd0\u52a8\u4f1a\u3002\u53cc\u65b9\u79ef\u6781\u652f\u6301\u4e24\u56fd\u4f53\u80b2\u673a\u6784\uff0c\u5305\u62ec\u5730\u65b9\u4f53\u80b2\u673a\u6784\u5f00\u5c55\u5bcc\u6709\u6210\u679c\u7684\u5408\u4f5c\u548c\u5efa\u7acb\u76f4\u63a5\u8054\u7cfb\u3002\u53cc\u65b9\u613f\u5f00\u5c55\u4f53\u80b2\u79d1\u7814\u5408\u4f5c\uff0c\u5e76\u4e3a\u6210\u529f\u4e3e\u529e\u5317\u4eac2022\u5e74\u51ac\u5965\u4f1a\u52a0\u5f3a\u51ac\u5b63\u4f53\u80b2\u9879\u76ee\u4ea4\u6d41\uff0c\u5171\u540c\u4e3e\u884c\u201c\u4e1d\u7ef8\u4e4b\u8def\u201d\u56fd\u9645\u62c9\u529b\u8d5b\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5c06\u8fdb\u4e00\u6b65\u7b80\u5316\u4eba\u5458\u5f80\u6765\u624b\u7eed\uff0c\u4fbf\u5229\u4e24\u56fd\u516c\u6c11\u65c5\u6e38\uff0c\u5f00\u5c55\u7ea2\u8272\u65c5\u6e38\u5408\u4f5c\uff0c\u9f13\u52b1\u5730\u65b9\u5f00\u5c55\u8de8\u5883\u65c5\u6e38\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u652f\u6301\u4e24\u56fd\u5e7f\u64ad\u3001\u7535\u89c6\u9886\u57df\u5408\u4f5c\uff0c\u652f\u6301\u4e24\u56fd\u901a\u8baf\u793e\u3001\u62a5\u793e\u5408\u4f5c\uff0c\u5171\u540c\u5b9e\u65bd\u7ffb\u8bd1\u548c\u51fa\u7248\u4e2d\u4fc4\u7ecf\u5178\u548c\u73b0\u4ee3\u6587\u5b66\u8457\u4f5c\u8ba1\u5212\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5c06\u7ee7\u7eed\u4e92\u529e\u7535\u5f71\u8282\uff0c\u9f13\u52b1\u5f15\u8fdb\u3001\u53d1\u884c\u3001\u64ad\u653e\u5bf9\u65b9\u5f71\u7247\uff0c\u652f\u6301\u5408\u62cd\u7535\u5f71\uff0c\u5e76\u5c06\u5c31\u7b7e\u7f72\u5408\u62cd\u7535\u5f71\u653f\u5e9c\u95f4\u534f\u5b9a\u7ee7\u7eed\u5f00\u5c55\u76f8\u5173\u51c6\u5907\u5de5\u4f5c\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5c06\u6269\u5927\u6863\u6848\u9886\u57df\u5408\u4f5c\uff0c\u652f\u6301\u4e3e\u529e\u6863\u6848\u6587\u732e\u5c55\u89c8\u3001\u6587\u732e\u51fa\u7248\u3001\u6863\u6848\u4fe1\u606f\u4ea4\u6d41\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5c06\u7ee7\u7eed\u4e3e\u529e\u767e\u540d\u9752\u5e74\u4ee3\u8868\u56e2\u4e92\u8bbf\uff0c\u652f\u6301\u4e2d\u4fc4\u9752\u5e74\u4f01\u4e1a\u5bb6\u4ff1\u4e50\u90e8\u3001\u5730\u65b9\u9752\u5e74\u7ec4\u7ec7\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u4e03',
                  u'\u3000\u3000\u5728\u53cc\u65b9\u5171\u540c\u52aa\u529b\u4e0b\uff0c\u5728\u76f8\u4e92\u5c0a\u91cd\u4e3b\u6743\u548c\u9886\u571f\u5b8c\u6574\u4ee5\u53ca\u4fdd\u6301\u56fd\u754c\u7ebf\u4e0d\u53d8\u539f\u5219\u57fa\u7840\u4e0a\uff0c\u4e2d\u4fc4\u56fd\u754c\u7b2c\u4e00\u6b21\u8054\u5408\u68c0\u67e5\u5de5\u4f5c\u8fdb\u5c55\u987a\u5229\u3002\u53cc\u65b9\u91cd\u7533\u5c06\u5728\u8ba1\u5212\u65f6\u9650\u5185\u5b8c\u6210\u5168\u90e8\u68c0\u67e5\u5de5\u4f5c\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u9ad8\u5ea6\u8bc4\u4ef7\u5728\u5408\u7406\u5229\u7528\u548c\u4fdd\u62a4\u8de8\u754c\u6c34\u8d44\u6e90\u65b9\u9762\u53d6\u5f97\u7684\u79ef\u6781\u6210\u679c\uff0c\u5e76\u5c06\u7ee7\u7eed\u6df1\u5316\u5728\u8de8\u754c\u6c34\u4f53\u6c34\u8d28\u76d1\u6d4b\u3001\u9632\u6d2a\u51cf\u707e\u3001\u8de8\u754c\u6c34\u8d44\u6e90\u4fdd\u62a4\u548c\u7ba1\u7406\u65b9\u9762\u7684\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u516b',
                  u'\u3000\u3000\u4e2d\u4fc4\u5c06\u7ee7\u7eed\u6df1\u5316\u5728\u4e0a\u6d77\u5408\u4f5c\u7ec4\u7ec7\u6846\u67b6\u5185\u7684\u5bc6\u5207\u534f\u8c03\u4e0e\u5408\u4f5c\uff0c\u575a\u5b9a\u63a8\u52a8\u63d0\u5347\u4e0a\u6d77\u5408\u4f5c\u7ec4\u7ec7\u5728\u7ef4\u62a4\u5730\u533a\u5b89\u5168\u7a33\u5b9a\u3001\u4fc3\u8fdb\u5171\u540c\u7e41\u8363\u65b9\u9762\u53d1\u6325\u7684\u4f5c\u7528\uff0c\u9020\u798f\u4e24\u56fd\u53ca\u672c\u5730\u533a\u4eba\u6c11\u3002\u53cc\u65b9\u652f\u6301\u5728\u5b89\u5168\u3001\u7ecf\u6d4e\u5408\u4f5c\u548c\u4eba\u6587\u4ea4\u6d41\u4e09\u4e2a\u4f18\u5148\u5408\u4f5c\u9886\u57df\u6df1\u5316\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u4e2d\u4fc4\u9ad8\u5ea6\u5173\u6ce8\u533a\u57df\u7ecf\u6d4e\u4e00\u4f53\u5316\uff0c\u5c06\u7ee7\u7eed\u843d\u5b9e\u4e2d\u56fd\u56fd\u5bb6\u4e3b\u5e2d\u4e60\u8fd1\u5e73\u548c\u4fc4\u7f57\u65af\u603b\u7edf\u666e\u4eac\u4e8e2015\u5e745\u67088\u65e5\u5171\u540c\u7b7e\u7f72\u7684\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u4e0e\u4fc4\u7f57\u65af\u8054\u90a6\u5173\u4e8e \u4e1d\u7ef8\u4e4b\u8def\u7ecf\u6d4e\u5e26\u5efa\u8bbe\u548c\u6b27\u4e9a\u7ecf\u6d4e\u8054\u76df\u5efa\u8bbe\u5bf9\u63a5\u5408\u4f5c\u7684\u8054\u5408\u58f0\u660e\u300b\u3002\u53cc\u65b9\u8ba4\u4e3a\u4e0a\u6d77\u5408\u4f5c\u7ec4\u7ec7\u662f\u5b9e\u73b0\u4e1d\u7ef8\u4e4b\u8def\u7ecf\u6d4e\u5e26\u5efa\u8bbe\u4e0e\u6b27\u4e9a\u7ecf\u6d4e\u8054\u76df\u5efa\u8bbe\u5bf9\u63a5\u7684\u6700\u6709\u6548\u5e73\u53f0\uff0c\u613f\u540c\u5176 \u4ed6\u56fd\u5bb6\u4e00\u9053\uff0c\u6700\u5927\u9650\u5ea6\u5229\u7528\u4e0a\u6d77\u5408\u4f5c\u7ec4\u7ec7\u7684\u73b0\u6709\u53d1\u5c55\u6f5c\u529b\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u575a\u4fe1\uff0c\u4e0a\u6d77\u5408\u4f5c\u7ec4\u7ec7\u6269\u5458\u7684\u51b3\u5b9a\u5c06\u4e3a\u589e\u5f3a\u7ec4\u7ec7\u5b9e\u529b\u3001\u62d3\u5c55\u6d3b\u52a8\u8303\u56f4\u548c\u63d0\u5347\u6210\u5458\u56fd\u4e4b\u95f4\u7684\u5408\u4f5c\u6c34\u5e73\u63d0\u4f9b\u65b0\u673a\u9047\u3002',
                  u'\u3000\u3000\u4e5d',
                  u'\u3000\u3000\u53cc\u65b9\u4e00\u81f4\u8ba4\u4e3a\uff0c\u56fd\u9645\u5f62\u52bf\u4e2d\u4e0d\u7a33\u5b9a\u3001\u4e0d\u786e\u5b9a\u56e0\u7d20\u589e\u591a\u3002\u9274\u4e8e\u6b64\uff0c\u53cc\u65b9\u5c06\u4f9d\u7167\u300a\u8054\u5408\u56fd\u5baa\u7ae0\u300b\u5b97\u65e8\u539f\u5219\u548c\u56fd\u9645\u6cd5\u57fa\u672c\u51c6\u5219\uff0c\u575a\u6301\u8054\u5408\u56fd\u7684\u6838\u5fc3\u4f5c\u7528\uff0c\u5171\u540c\u81f4 \u529b\u4e8e\u6784\u5efa\u4ee5\u5408\u4f5c\u5171\u8d62\u4e3a\u6838\u5fc3\u7684\u65b0\u578b\u56fd\u9645\u5173\u7cfb\u3002\u4e2d\u4fc4\u5c06\u63a8\u52a8\u5efa\u7acb\u66f4\u52a0\u7a33\u5b9a\u7684\u56fd\u9645\u5173\u7cfb\u683c\u5c40\uff0c\u7ef4\u62a4\u5e7f\u5927\u53d1\u5c55\u4e2d\u56fd\u5bb6\u5171\u540c\u5229\u76ca\uff0c\u53cd\u6620\u73b0\u4ee3\u4e16\u754c\u6587\u660e\u591a\u6837\u6027\uff0c\u5e76\u4e3a\u5f00\u5c55\u5e7f\u6cdb\u56fd \u9645\u5408\u4f5c\u521b\u9020\u6761\u4ef6\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u6307\u51fa\uff0c\u4e16\u754c\u7ecf\u6d4e\u4ecd\u5904\u4e8e\u91d1\u878d\u5371\u673a\u540e\u7684\u7f13\u6162\u590d\u82cf\u671f\uff0c\u4e0b\u884c\u538b\u529b\u52a0\u5927\uff0c\u7ecf\u6d4e\u590d\u82cf\u57fa\u7840\u4ecd\u4e0d\u7262\u56fa\u3002\u53cc\u65b9\u652f\u6301\u5f00\u653e\u578b\u4e16\u754c\u7ecf\u6d4e\u548c\u56fd\u9645\u8d38\u6613\u3001\u6295\u8d44\u81ea\u7531\u5316\u548c\u4fbf\u5229 \u5316\uff0c\u7ef4\u62a4\u56fd\u9645\u91d1\u878d\u7a33\u5b9a\uff0c\u786e\u4fdd\u4e16\u754c\u7ecf\u6d4e\u6301\u7eed\u3001\u5747\u8861\u3001\u7a33\u5b9a\u589e\u957f\u3002\u56fd\u9645\u793e\u4f1a\u5e94\u7ee7\u7eed\u52a0\u5f3a\u5b8f\u89c2\u7ecf\u6d4e\u653f\u7b56\u534f\u8c03\uff0c\u63a8\u8fdb\u7ed3\u6784\u6027\u6539\u9769\u548c\u56fd\u9645\u91d1\u878d\u4f53\u7cfb\u6539\u9769\uff0c\u4ee5\u53ca\u5728\u4e8c\u5341\u56fd\u96c6\u56e2\u7b49 \u673a\u5236\u4e0b\u5b8c\u5584\u5168\u7403\u7ecf\u6d4e\u6cbb\u7406\u3002\u4fc4\u65b9\u8868\u793a\u5c06\u652f\u6301\u4e2d\u65b9\u4e3b\u529e2016\u5e74\u4e8c\u5341\u56fd\u96c6\u56e2\u5cf0\u4f1a\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u5f3a\u8c03\uff0c\u6050\u6016\u4e3b\u4e49\u662f\u5168\u4eba\u7c7b\u5171\u540c\u5a01\u80c1\u3002\u56fd\u9645\u793e\u4f1a\u5e94\u7262\u56fa\u6811\u7acb\u547d\u8fd0\u5171\u540c\u4f53\u610f\u8bc6\uff0c\u9075\u5faa\u300a\u8054\u5408\u56fd\u5baa\u7ae0\u300b\u5b97\u65e8\u548c\u539f\u5219\uff0c\u5728\u8054\u5408\u56fd\u53d1\u6325\u6838\u5fc3\u534f\u8c03\u4f5c\u7528\u60c5\u51b5\u4e0b\uff0c\u91c7 \u53d6\u653f\u6cbb\u3001\u7ecf\u6d4e\u3001\u5916\u4ea4\u53ca\u5fc5\u8981\u7684\u519b\u4e8b\u7b49\u7efc\u5408\u624b\u6bb5\uff0c\u52a0\u5f3a\u534f\u8c03\uff0c\u6807\u672c\u517c\u6cbb\uff0c\u5171\u540c\u5e94\u5bf9\u6050\u6016\u4e3b\u4e49\u3002\u4e2d\u65b9\u652f\u6301\u4fc4\u7f57\u65af\u53ca\u5176\u4ed6\u5730\u533a\u56fd\u5bb6\u6253\u51fb\u6050\u6016\u4e3b\u4e49\u7684\u52aa\u529b\u3002\u53cc\u65b9\u5c06\u7ee7\u7eed\u5168\u9762\u52a0 \u5f3a\u53cc\u3001\u591a\u8fb9\u53cd\u6050\u9886\u57df\u7684\u653f\u7b56\u534f\u8c03\u4e0e\u5efa\u8bbe\u6027\u5408\u4f5c\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u91cd\u89c6\u5e76\u652f\u6301\u653f\u6cbb\u89e3\u51b3\u53d9\u5229\u4e9a\u95ee\u9898\uff0c\u652f\u6301\u8054\u5408\u56fd\u65a1\u65cb\u52aa\u529b\uff0c\u5e76\u53d1\u6325\u8054\u5408\u56fd\u7684\u4e3b\u6e20\u9053\u4f5c\u7528\u3002\u53cc\u65b9\u540c\u610f\u5c31\u53d9\u5229\u4e9a\u95ee\u9898\u7ee7\u7eed\u4fdd\u6301\u5bc6\u5207\u6c9f\u901a\u4e0e\u534f\u8c03\uff0c\u63a8\u52a8\u5404\u65b9\u5c3d\u5feb\u91cd\u542f\u4e0d\u8bbe\u7f6e\u524d\u63d0\u3001\u4e0d\u9884\u65ad\u7ed3\u679c\uff0c\u5e76\u4e14\u5177\u6709\u5305\u5bb9\u6027\u7684\u653f\u6cbb\u8fc7\u6e21\u8fdb\u7a0b\uff0c\u6700\u7ec8\u7531\u53d9\u5229\u4e9a\u4eba\u6c11\u4ee5\u81ea\u4e3b\u81ea\u613f\u7684\u65b9\u5f0f\uff0c\u51b3\u5b9a\u56fd\u5bb6\u7684\u672a\u6765\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u91cd\u7533\uff0c\u7ef4\u62a4\u671d\u9c9c\u534a\u5c9b\u548c\u5e73\u7a33\u5b9a\uff0c\u5b9e\u73b0\u534a\u5c9b\u65e0\u6838\u5316\uff0c\u901a\u8fc7\u5bf9\u8bdd\u534f\u5546\u89e3\u51b3\u6709\u5173\u95ee\u9898\u7b26\u5408\u516d\u65b9\u4f1a\u8c08\u5404\u53c2\u4e0e\u65b9\u5171\u540c\u5229\u76ca\u3002\u516d\u65b9\u4f1a\u8c08\u662f\u89e3\u51b3\u671d\u6838\u95ee\u9898\u73b0\u5b9e\u6709\u6548\u7684 \u9014\u5f84\uff0c\u6709\u5173\u5404\u65b9\u5e94\u76f8\u5411\u800c\u884c\uff0c\u5168\u9762\u843d\u5b9e\u516d\u65b9\u4f1a\u8c082005\u5e74\u201c9\xb719\u201d\u5171\u540c\u58f0\u660e\uff0c\u4e3a\u63a8\u52a8\u65e9\u65e5\u91cd\u542f\u516d\u65b9\u4f1a\u8c08\uff0c\u5b9e\u73b0\u534a\u5c9b\u548c\u4e1c\u5317\u4e9a\u5730\u533a\u7684\u957f\u6cbb\u4e45\u5b89\u800c\u5171\u540c\u52aa\u529b\u3002\u53cc\u65b9 \u5c06\u7ee7\u7eed\u5c31\u671d\u9c9c\u534a\u5c9b\u6838\u95ee\u9898\u5f00\u5c55\u5bc6\u5207\u534f\u8c03\u4e0e\u534f\u4f5c\uff0c\u81f4\u529b\u4e8e\u5728\u4e1c\u5317\u4e9a\u5730\u533a\u5efa\u7acb\u6709\u6548\u7684\u548c\u5e73\u4e0e\u5b89\u5168\u673a\u5236\u3002',
                  u'\u3000\u3000\u53cc\u65b9\u6b22\u8fce\u4f0a\u6717\u6838\u95ee\u9898\u300a\u8054\u5408\u5168\u9762\u884c\u52a8\u8ba1\u5212\u300b\u8fbe\u6210\u5e76\u6b63\u5f0f\u751f\u6548\uff0c\u8ba4\u4e3a\u4e0a\u8ff0\u5171\u8bc6\u7684\u8fbe\u6210\u6709\u52a9\u4e8e\u7ef4\u62a4\u56fd\u9645\u6838\u4e0d\u6269\u6563\u4f53\u7cfb\uff0c\u6709\u52a9\u4e8e\u4fc3\u8fdb\u5730\u533a\u548c\u5e73\u7a33\u5b9a\uff0c\u662f\u901a\u8fc7\u653f\u6cbb \u5916\u4ea4\u624b\u6bb5\u89e3\u51b3\u56fd\u9645\u548c\u5730\u533a\u70ed\u70b9\u95ee\u9898\u7684\u6210\u529f\u8303\u4f8b\u3002\u53cc\u65b9\u547c\u5401\u6709\u5173\u5404\u65b9\u79c9\u6301\u8bda\u610f\uff0c\u5207\u5b9e\u5c65\u884c\u627f\u8bfa\uff0c\u5168\u9762\u5e73\u8861\u6267\u884c\u4e0a\u8ff0\u5171\u8bc6\u53ca\u5b89\u7406\u4f1a\u7b2c2231\u53f7\u51b3\u8bae\u3002\u53cc\u65b9\u540c\u610f\u5728\u843d\u5b9e\u4e0a \u8ff0\u5171\u8bc6\u8fc7\u7a0b\u4e2d\u52a0\u5f3a\u6c9f\u901a\u4e0e\u5408\u4f5c\uff0c\u7ee7\u7eed\u4e3a\u5168\u9762\u3001\u957f\u671f\u3001\u59a5\u5584\u89e3\u51b3\u4f0a\u6717\u6838\u95ee\u9898\u53d1\u6325\u5efa\u8bbe\u6027\u4f5c\u7528\u3002',
                  u'\u3000\u3000\u5341',
                  u'\u3000\u3000\u4e2d\u4fc4\u603b\u7406\u7b2c\u4e8c\u5341\u6b21\u5b9a\u671f\u4f1a\u6664\u671f\u95f4\uff0c\u53cc\u65b9\u7b7e\u7f72\u4e86\u4ee5\u4e0b\u6587\u4ef6\uff1a',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u4fc4\u603b\u7406\u7b2c\u4e8c\u5341\u6b21\u5b9a\u671f\u4f1a\u6664\u8054\u5408\u516c\u62a5\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u4fc4\u6295\u8d44\u5408\u4f5c\u59d4\u5458\u4f1a\u7b2c\u4e8c\u6b21\u4f1a\u8bae\u7eaa\u8981\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u4fc4\u80fd\u6e90\u5408\u4f5c\u59d4\u5458\u4f1a\u7b2c\u5341\u4e8c\u6b21\u4f1a\u8bae\u7eaa\u8981\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u4fc4\u4eba\u6587\u5408\u4f5c\u59d4\u5458\u4f1a\u7b2c\u5341\u516d\u6b21\u4f1a\u8bae\u7eaa\u8981\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u4fc4\u603b\u7406\u5b9a\u671f\u4f1a\u6664\u59d4\u5458\u4f1a\u7b2c\u5341\u4e5d\u6b21\u4f1a\u8bae\u7eaa\u8981\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u4fc4\u53cb\u597d\u3001\u548c\u5e73\u4e0e\u53d1\u5c55\u59d4\u5458\u4f1a2016\u5e74\u5de5\u4f5c\u8ba1\u5212\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u5173\u4e8e\u4e00\u4e5d\u4e5d\u4e03\u5e74\u516d\u6708\u4e8c\u5341\u4e03\u65e5\u3008\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u653f\u5e9c\u548c\u4fc4\u7f57\u65af\u8054\u90a6\u653f\u5e9c\u5173\u4e8e\u5efa\u7acb\u4e2d\u4fc4\u603b\u7406\u5b9a\u671f\u4f1a\u6664\u673a\u5236\u53ca\u5176\u7ec4\u7ec7\u539f\u5219\u7684\u534f\u5b9a\u3009\u7684\u8bae\u5b9a\u4e66\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u3008\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u653f\u5e9c\u4e0e\u4fc4\u7f57\u65af\u8054\u90a6\u653f\u5e9c\u5173\u4e8e\u5b9e\u65bd\u4e9a\u9a6c\u5c14\u6db2\u5316\u5929\u7136\u6c14\u9879\u76ee\u7684\u5408\u4f5c\u534f\u8bae\u3009\u7684\u8bae\u5b9a\u4e66\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u56fd\u5bb6\u53d1\u5c55\u548c\u6539\u9769\u59d4\u5458\u4f1a\u4e0e\u4fc4\u7f57\u65af\u8054\u90a6\u8fdc\u4e1c\u53d1\u5c55\u90e8\u5173\u4e8e\u52a0\u5f3a\u4e2d\u4fc4\u8fdc\u4e1c\u533a\u57df\u3001\u4ea7\u4e1a\u53ca\u6295\u8d44\u5408\u4f5c\u7684\u8c05\u89e3\u5907\u5fd8\u5f55\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u6559\u80b2\u90e8\u4e0e\u4fc4\u7f57\u65af\u8054\u90a6\u6587\u5316\u90e8\u5728\u6587\u5316\u9886\u57df\u5f00\u5c55\u6559\u80b2\u548c\u79d1\u5b66\u5408\u4f5c\u7684\u8c05\u89e3\u5907\u5fd8\u5f55\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u79d1\u5b66\u6280\u672f\u90e8\u3001\u4fc4\u7f57\u65af\u8054\u90a6\u6559\u80b2\u79d1\u5b66\u90e8\u3001\u4e2d\u56fd\u79d1\u5b66\u9662\u548c\u8054\u5408\u6838\u5b50\u7814\u7a76\u6240\u5173\u4e8e\u5728\u91cd\u79bb\u5b50\u8d85\u5bfc\u540c\u6b65\u52a0\u901f\u5668\u6846\u67b6\u4e0b\u5408\u4f5c\u524d\u666f\u7684\u8bae\u5b9a\u4e66\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u8d22\u653f\u90e8\u4e0e\u4fc4\u7f57\u65af\u8054\u90a6\u8d22\u653f\u90e8\u8d22\u91d1\u9886\u57df\u5408\u4f5c\u8c05\u89e3\u5907\u5fd8\u5f55\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u4ea4\u901a\u8fd0\u8f93\u90e8\u548c\u4fc4\u7f57\u65af\u8054\u90a6\u8fd0\u8f93\u90e8\u5173\u4e8e\u4e2d\u4fc4\u8d27\u8fd0\u8f66\u8f86\u7ecf\u54c8\u8428\u514b\u65af\u5766\u9886\u571f\u4e34\u65f6\u8fc7\u5883\u8d27\u7269\u8fd0\u8f93\u534f\u8bae\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u5546\u52a1\u90e8\u548c\u4fc4\u7f57\u65af\u8054\u90a6\u7ecf\u6d4e\u53d1\u5c55\u90e8\u5173\u4e8e\u4fc3\u8fdb\u53cc\u8fb9\u8d38\u6613\u53d1\u5c55\u7684\u8c05\u89e3\u5907\u5fd8\u5f55\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u5546\u52a1\u90e8\u3001\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u9ed1\u9f99\u6c5f\u7701\u4eba\u6c11\u653f\u5e9c\u4e0e\u4fc4\u7f57\u65af\u8054\u90a6\u7ecf\u6d4e\u53d1\u5c55\u90e8\u3001\u4fc4\u7f57\u65af\u8054\u90a6\u5de5\u4e1a\u8d38\u6613\u90e8\u5173\u4e8e\u7b79\u5907\u548c\u4e3e\u529e\u4e2d\u4fc4\u535a\u89c8\u4f1a\u7684\u8c05\u89e3\u5907\u5fd8\u5f55\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u6d77\u5173\u603b\u7f72\u548c\u4fc4\u7f57\u65af\u8054\u90a6\u6d77\u5173\u7f72\u5173\u4e8e\u5e72\u90e8\u6559\u80b2\u57f9\u8bad\u5408\u4f5c\u7684\u5907\u5fd8\u5f55\u300b',
                  u'\u3000\u3000\u2014\u2014\u300a\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u56fd\u5bb6\u8d28\u91cf\u76d1\u7763\u68c0\u9a8c\u68c0\u75ab\u603b\u5c40\u548c\u4fc4\u7f57\u65af\u8054\u90a6\u517d\u533b\u548c\u690d\u7269\u536b\u751f\u76d1\u7763\u5c40\u5173\u4e8e\u4fc4\u7f57\u65af\u5c0f\u9ea6\u8f93\u534e\u690d\u7269\u68c0\u75ab\u8981\u6c42\u8bae\u5b9a\u4e66\u300b',
